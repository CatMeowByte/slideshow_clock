; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Slideshow Clock"
config/description="Dashboard clock with muslim praytimes and slideshow."
config/version="1.0.1"
run/main_scene="res://scene/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="catmeowbyte/slideshow_clock"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/low_processor_mode=true
run/low_processor_mode_sleep_usec=33333
boot_splash/bg_color=Color(0.254902, 0.65098, 0.964706, 1)
boot_splash/image="res://image/catmeowbyte_256.png"
boot_splash/fullsize=false
config/icon="res://image/icon.svg"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="canvas_items"

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[internationalization]

locale/translations=PackedStringArray("res://locale/locale.en.translation", "res://locale/locale.id.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_s3tc_bptc=true
